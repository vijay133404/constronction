<div class="gallery" id="gallery">
   <div class="container">
   <h3 class="tittle">Our <span>Projects</span></h3>
     <div class="gallery-grids">
     <section>
       <ul id="da-thumbs" class="da-thumbs">
         
    			<%@galleries.each do |image| %>
    			<%unless image.image.url.blank?%>
    			  <li>
    			      <%= link_to image_tag image.image.url(:thumb), alt: image.image.url(:thumb) %>
    			  </li>
    			<% end %> 
    			<% end %>
		 
      </ul>
      </section>
    </div>
  </div>
</div>			